pipeline {
    agent any
    stages {
        stage ('Maven'){
            agent {
                docker {image 'maven:3.9.11-eclipse-temurin-11'}
	    }
	    steps {
                sh 'mvn --version'
            }
        }
        stage ('Node'){
            agent {
                docker {image 'node:16-alpine'}
            }   
            steps {
                sh 'node --version'
            }
        }
    }

}
